      <lib_task_param id="4" name="command" description="Command Line" modifiable="Yes" explode="None">
         <lib_task_param_default value="echo ${VL_CONTENT_NAME} ${VL_CONTENT_PATH}" lib_task_param_id="4"/>
      </lib_task_param>
   </lib_task>
</vl_activity_library>
